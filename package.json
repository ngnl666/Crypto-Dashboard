{
	"name": "crypto-dashboard",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "nuxt dev --open",
		"serve": "nuxt dev --open",
		"build": "vue-tsc --noEmit && nuxt build",
		"test:unit": "vitest watch",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --open --coverage",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"prepare": "husky install",
		"lint:eslint": "eslint --fix --ext .js,.ts,.cjs,.vue",
		"lint:prettier": "prettier --write --loglevel warn \"*.{js,ts,cjs,json,tsx,css,less,scss,vue,html,md}\"",
		"lint:lint-staged": "lint-staged",
		"commit": "git add -A && git-cz",
		"postinstall": "nuxt prepare"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.0.10",
		"@pinia/nuxt": "^0.4.6",
		"echarts": "^5.4.0",
		"element-plus": "^2.2.25",
		"pinia-plugin-persistedstate": "^3.0.1",
		"vue-echarts": "^6.2.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@iconify-json/ep": "^1.1.8",
		"@nuxt/test-utils-edge": "^3.0.1-rc.0-27828744.aa9aec1",
		"@nuxtjs/eslint-config-typescript": "^12.0.0",
		"@nuxtjs/tailwindcss": "^6.1.3",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-c8": "^0.25.3",
		"@vitest/ui": "^0.25.3",
		"@vue/test-utils": "^2.2.4",
		"commitizen": "^4.2.5",
		"cz-git": "^1.3.12",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.8.0",
		"husky": "^8.0.2",
		"jsdom": "^20.0.3",
		"lint-staged": "^13.0.4",
		"nuxt": "3.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-tailwindcss": "^0.2.0",
		"rollup-plugin-visualizer": "^5.8.3",
		"typescript": "^4.9.3",
		"unplugin-auto-import": "^0.12.0",
		"unplugin-icons": "^0.14.14",
		"unplugin-vue-components": "^0.22.11",
		"vitest": "^0.25.3",
		"vue-tsc": "^1.0.10"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	}
}
